<form method="post" action="{{ path('app_resultat_delete', {'id': resultat.id}) }}" onsubmit="return confirm('Êtes-vous sûr(e) de vouloir supprimer cet resultat ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ resultat.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
